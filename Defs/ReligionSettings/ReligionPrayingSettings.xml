<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ReligionsOfRimworld.ReligionSettingsDef Name="RoRPrayingSettingsBase" Abstract="true">
		<settings Class="ReligionsOfRimworld.ReligionSettings_Prayings">
			<tag>PrayingsTag</tag>
		</settings>
	</ReligionsOfRimworld.ReligionSettingsDef>

	<ReligionsOfRimworld.ReligionSettingsDef ParentName="RoRPrayingSettingsBase">
		<defName>RoRPrayingSettings</defName>
		<settings Class="ReligionsOfRimworld.ReligionSettings_Prayings">
			<prayJob>TestPray</prayJob>
			<prayProperty>
				<piety>PrayPietyTest</piety>
			</prayProperty>
			<prayIntervalHours>4</prayIntervalHours>
		</settings>
	</ReligionsOfRimworld.ReligionSettingsDef>

	<ReligionsOfRimworld.PietyDef>
		<defName>PrayPietyTest</defName>
		<stages>
	      <li>
	        <label>very low</label>
	        <description>sad</description>
	        <pietyRate>1.3</pietyRate>
	      </li>
	      <li>
	        <label>low</label>
	        <description>sad</description>
	        <pietyRate>1.3</pietyRate>
	      </li>
	      <li>
	        <label>ok</label>
	        <description>sad</description>
	        <pietyRate>1.3</pietyRate>
	      </li>
	      <li>
	        <label>good</label>
	        <description>sad</description>
	        <pietyRate>1.3</pietyRate>
	      </li>
	      <li>
	        <label>verygood</label>
	        <description>sad</description>
	        <pietyRate>1.3</pietyRate>
	      </li>
		</stages>
	</ReligionsOfRimworld.PietyDef>
</Defs>