<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ReligionsOfRimworld.ReligionSettingsDef Name="RoRWorshipSettingsBase" Abstract="true">
		<settings Class="ReligionsOfRimworld.ReligionSettings_ReligionActivity">
			<tag>WorshipTag</tag>
		</settings>
	</ReligionsOfRimworld.ReligionSettingsDef>

	<ReligionsOfRimworld.ReligionSettingsDef ParentName="RoRWorshipSettingsBase">
		<defName>RoRWorshipSettingsTest</defName>
		<settings Class="ReligionsOfRimworld.ReligionSettings_ReligionActivity">	
			<properties>
				<li>
          <activityJobQueue>UwU</activityJobQueue>
					<recipe>Testt</recipe>
				</li>
			</properties>
		</settings>
	</ReligionsOfRimworld.ReligionSettingsDef>

  <ReligionsOfRimworld.ActivityJobQueueDef>
    <defName>UwU</defName>
    <activityNodes>
      <li>
        <organizerJob>ReadPrayers</organizerJob>
      </li>
      <li>
        <congregationJob>ListenPrayers</congregationJob>
      </li>
    </activityNodes>
  </ReligionsOfRimworld.ActivityJobQueueDef>

	<RecipeDef ParentName="CookMealBase">
    <defName>Testt</defName>
    <label>cook simple meal</label>
    <description>Cook a straightforward meal from one ingredient.</description>
    <jobString>Cooking simple meal.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>FoodRaw</li>
          </categories>
        </filter>
        <count>0.5</count>
      </li>
    </ingredients>
  </RecipeDef>
</Defs>
